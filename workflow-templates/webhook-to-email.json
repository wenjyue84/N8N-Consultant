{
  "name": "Webhook to Email",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "webhook",
        "responseMode": "responseNode"
      },
      "id": "webhook-node",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300],
      "webhookId": "webhook-id-1"
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "subject",
              "value": "=New webhook received"
            },
            {
              "name": "body",
              "value": "={{ JSON.stringify($json) }}"
            }
          ]
        },
        "options": {}
      },
      "id": "set-node",
      "name": "Prepare Email Data",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [450, 300]
    },
    {
      "parameters": {
        "fromEmail": "noreply@example.com",
        "toEmail": "recipient@example.com",
        "subject": "={{ $json.subject }}",
        "text": "={{ $json.body }}",
        "options": {}
      },
      "id": "email-node",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "typeVersion": 1,
      "position": [650, 300],
      "credentials": {
        "smtp": {
          "id": "1",
          "name": "SMTP account"
        }
      }
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ { \"status\": \"success\", \"message\": \"Email sent\" } }}"
      },
      "id": "respond-node",
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [850, 300]
    }
  ],
  "connections": {
    "webhook-node": {
      "main": [
        [
          {
            "node": "set-node",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "set-node": {
      "main": [
        [
          {
            "node": "email-node",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "email-node": {
      "main": [
        [
          {
            "node": "respond-node",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
